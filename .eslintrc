{
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "comma-spacing": 0,
    "consistent-return": 0,
    "indent": [2, 2],
    "key-spacing": 0,
    "no-multi-spaces": 0,
    "no-shadow": 0,
    "strict": [0, "never"],
    "quotes": [2, "single", "avoid-escape"],
    "yoda": 0
  }
}
